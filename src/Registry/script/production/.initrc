# see https://www.elastic.co/guide/en/elasticsearch/reference/current/indices-update-settings.html

[reports]
index      : reports_v1
alias      : reports
type       : report

[repository]
name       : backup
type       : fs
location   : /nfs/public/nobackup/ens_thr/production/elastic_dumps/es_snapshots
